         </tr>
         <tr>
             <td style="" colspan="4">  </td>
          </tr>
         <tr>
             <td style="" colspan="4">  </td>
          </tr>
            {% if element.typeusagerclient == 'Personne physique' %}
         <tr>
             <td style="width: 35mm; text-align: left; font-size: 10pt;" colspan="1">Nom et Prénom(s) : </td>
             <td style="width: 50mm; text-align: left; font-size: 10pt;font-weight: bold;" colspan="1"> {{  element.particulier.prenom }}   {{  element.particulier.nom }}  </td>
          
              <td style="width: 30mm; text-align: left; font-size: 10pt;" colspan="1">Contact(s) : </td>
             <td style="width: 45mm; text-align: left; font-size: 10pt;font-weight: bold;" colspan="1">  {{  element.particulier.usagerclient.adressecontact.telmobile }}  </td>
          </tr>
         <tr>
             <td style="" colspan="4">  </td>
          </tr>
           <tr>
             <td style="width: 35mm; text-align: left; font-size: 10pt;" colspan="1">E@mail : </td>
             <td style="width: 50mm; text-align: left; font-size: 10pt;font-weight: bold;" colspan="1">   {{  element.particulier.usagerclient.adressecontact.email }}  </td>
          
              <td style="width: 30mm; text-align: left; font-size: 10pt;" colspan="1">Département : </td>
             <td style="width: 45mm; text-align: left; font-size: 10pt;font-weight: bold;" colspan="1">    {{  element.particulier.usagerclient.adressecontact.departement.nomdepartement }}  </td>
