             <td style="width: 50mm; text-align: left; font-size: 10pt;font-weight: bold;" colspan="1">  {{  element.societeentreprise.usagerclient.adressecontact.email }}  </td>
          
              <td style="width: 30mm; text-align: left; font-size: 10pt;" colspan="1">Contact(s) : </td>
